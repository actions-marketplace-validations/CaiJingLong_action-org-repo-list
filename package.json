{
  "name": "action-make-org-repo-list",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub Action to make a list of all repos in an organization",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "make-example": "pnpm run build && node lib/make_example.js",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "pnpm install && pnpm exec jest",
    "dist": "pnpm run build && pnpm run package",
    "all": "pnpm lint && pnpm test && pnpm dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/core": "^4.2.0",
    "@octokit/rest": "^19.0.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^20.4.1",
    "@typescript-eslint/parser": "^5.57.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.37.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}